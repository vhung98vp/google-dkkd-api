            if (isEmpty) $(".alert-container").show();
            //end_thaont
            var accnType = $("#ctl00_C_ANNOUNCEMENT_TYPE_IDFilterFld").val();
            if (accnType == "") isEmpty = true;

            if (isEmpty) $(".alert-container").show();
            else $(".alert-container").hide();
            return !isEmpty;
        }

