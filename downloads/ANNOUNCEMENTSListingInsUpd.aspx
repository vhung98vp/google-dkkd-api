            $(".alert-container").hide();
            $("#ctl00_C_PnlFilterBody").find("input:text").each(function () {
                $(this).val("");
            });
            $("#ctl00_C_PnlFilterBody").find("select").each(function () {
                $(this).attr("selected", "");
                $(this).val("");
            });

            return false;
        }
